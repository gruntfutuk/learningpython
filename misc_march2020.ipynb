   "metadata": {},
   "outputs": [],
   "source": [
    "@alias_class\n",
    "class Calculator:\n",
    "    @alias('+')\n",
    "    def add(self):\n",
    "        ...\n",
    "\n",
    "calculator = Calculator()\n",
    "calculator['+']()"
